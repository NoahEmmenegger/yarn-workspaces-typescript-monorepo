{
    "compilerOptions": {
        "composite": true,
        "target": "es5",
        "module": "commonjs",
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": ["dist/**", "tests/**"],
    "references": [
        {
            "path": "../config"
        }
    ]
}
